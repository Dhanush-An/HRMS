{
    "version": 2,
    "functions": {
        "backend/src/server.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "backend/src/server.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}